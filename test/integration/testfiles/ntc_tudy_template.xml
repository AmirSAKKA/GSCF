				<required>true</required>
				<preferredIdentifier>false</preferredIdentifier>

				
				
			</templateField>
		  
			<templateField>
				<name>StudyTitle</name>
				<type>STRING</type>
				<unit></unit>
				<comment>Short description of study</comment>
				<required>true</required>
				<preferredIdentifier>false</preferredIdentifier>

				
				
			</templateField>
		  
			<templateField>
				<name>StudyMainSpecies</name>
				<type>ONTOLOGYTERM</type>
				<unit></unit>
				<comment>Main species used in the study  (also try for in vitro and when mixing species)</comment>
				<required>true</required>
				<preferredIdentifier>false</preferredIdentifier>

				
				  <ontologies>
					
					  <ontology>
						  <ncboId>1132</ncboId>
						  <ncboVersionedId>38802</ncboVersionedId>
						  <name>NCBI organismal classification</name>
						  <description>A taxonomic classification of living organisms and associated artifacts for their controlled description within the context of databases.</description>
						  <url>http://bioportal.bioontology.org/ontologies/38802</url>
						  <versionNumber>1.2</versionNumber>
					  </ontology>
					
				  </ontologies>
				
				
			</templateField>
		  
			<templateField>
				<name>StudyMainSource</name>
				<type>ONTOLOGYTERM</type>
				<unit></unit>
				<comment>Give name of cells for in vitro (e.g. cell line) or body part for in vivo (e.g. organ or tissue)</comment>
				<required>true</required>
				<preferredIdentifier>false</preferredIdentifier>

				
				  <ontologies>
					
					  <ontology>
						  <ncboId>1005</ncboId>
						  <ncboVersionedId>40643</ncboVersionedId>
						  <name>BRENDA tissue / enzyme source</name>
						  <description>A structured controlled vocabulary for the source of an enzyme. It comprises terms for tissues, cell lines, cell types and cell cultures from uni- and multicellular organisms.</description>
						  <url>http://bioportal.bioontology.org/ontologies/40643</url>
						  <versionNumber>1.3</versionNumber>
					  </ontology>
					
				  </ontologies>
				
				
			</templateField>
		  
